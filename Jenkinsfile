pipeline{
    agent any
    stages {
        stage('Start') {
            steps{
                echo 'This is a start step' 
            }
        }
        stage('Build') {
            steps{
                echo 'This is a build step' 
            }
        }
        stage('Test') {
            steps{
                echo 'This is a test step'  
            }
        }
        stage('Deploy') {
            steps{
                echo 'This is a deploy step'    
            }
        }
        stage('End') {
            steps{
                echo 'This is a end step' 
            }
        }
    }
}
